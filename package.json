{
  "name": "mlit-geojson-to-rdf4j",
  "version": "1.0.0",
  "description": "CLI tool to convert MLIT GeoJSON data to GeoSPARQL RDF and load into RDF4J server",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "query": "node dist/query-tool.js",
    "query:dev": "ts-node src/query-tool.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "geosparql",
    "rdf4j",
    "geojson",
    "mlit",
    "semantic-web",
    "spatial-data"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "fetch-sparql-endpoint": "^4.1.1",
    "node-fetch": "^2.7.0",
    "proj4": "^2.9.2",
    "stream-json": "^1.8.0",
    "wellknown": "^0.5.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.16",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "@types/proj4": "^2.5.4",
    "@types/stream-json": "^1.7.7",
    "@types/wellknown": "^0.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
